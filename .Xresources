!! URxvt font
!URxvt*font: xft:Ubuntu Mono:style=Regular:pixelsize=20
!URxvt*boldFont: xft:Ubuntu Mono:style=Regular:pixelsize=20
!URxvt*italicFont: xft:Ubuntu Mono:style=Italic:pixelsize=20
!URxvt*boldItalicFont: xft:Ubuntu Mono:style=Bold Italic:pixelsize=20
!URxvt.letterSpace: -1

*.font: xft:Fixedsys Excelsior:style=Regular:pixelsize=15
*.boldFont: xft:Fixedsys Excelsior:style=Regular:pixelsize=18
*.letterSpace: -2

rofi.font: hack 10

!*.font: xft:Roboto Mono Medium for Powerline:style=Regular:pixelsize=16
!*.boldFont: xft:Roboto Mono Medium for Powerline:style=Regular:pixelsize=16
!*.letterSpace: 0

!! URxvt scrollbar
URxvt.scrollType:	rxvt
URxvt.scrollBar: 	false

!! URxvt cursor
URxvt.cursorBlink:	true
URxvt.cursorUnderline:	false

!! URxvt pointer
URxvt.pointerBlank:	true

!! URxvt transparency
URxvt.transparent:	false
URxvt.shading:		20

