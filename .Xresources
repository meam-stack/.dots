!! vim colorscheme default

!! URxvt font
!URxvt*font: xft:Ubuntu Mono:style=Regular:pixelsize=20
!URxvt*boldFont: xft:Ubuntu Mono:style=Regular:pixelsize=20
!URxvt*italicFont: xft:Ubuntu Mono:style=Italic:pixelsize=20
!URxvt*boldItalicFont: xft:Ubuntu Mono:style=Bold Italic:pixelsize=20
!URxvt.letterSpace: -1

*.font: xft:Fixedsys Excelsior:style=Regular:pixelsize=15:antialias=false
*.boldFont: xft:Fixedsys Excelsior:style=Regular:pixelsize=18:antialias=false
*.letterSpace: -1

rofi.font: hack 10

!! URxvt scrollbar
URxvt.scrollType:	rxvt
URxvt.scrollBar: 	false

!! URxvt cursor
URxvt.cursorBlink:	true
URxvt.cursorUnderline:	false

!! URxvt pointer
URxvt.pointerBlank:	true

!! URxvt transparency
URxvt.transparent:	true
URxvt.shading:		20

! special
*.foreground:   #93a1a1
*.background:   #002b36
*.cursorColor:  #93a1a1

*.foregroundAlt:   #268bd2 
*.backgroundAlt:   #002b36

! black
*.color0:       #002b36
*.color8:       #657b83

! red
*.color1:       #dc322f
*.color9:       #dc322f

! green
*.color2:       #859900
*.color10:      #859900

! yellow
*.color3:       #b58900
*.color11:      #b58900

! blue
*.color4:       #268bd2
*.color12:      #268bd2

! magenta
*.color5:       #6c71c4
*.color13:      #6c71c4

! cyan
*.color6:       #2aa198
*.color14:      #2aa198

! white
*.color7:       #93a1a1
*.color15:      #fdf6e3

